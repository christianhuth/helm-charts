        "registry": {
          "default": "docker.io",
          "description": "image registry",
          "required": [],
          "title": "registry",
          "type": "string"
        },
        "repository": {
          "default": "headwindmdm/hmdm",
          "description": "image repository",
          "required": [],
          "title": "repository",
          "type": "string"
        },
        "tag": {
          "default": "0.1.6",
