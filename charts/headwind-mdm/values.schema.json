            "existingSecret": {
              "default": "",
              "description": "Name of existing secret to use for PostgreSQL credentials",
              "required": [],
              "title": "existingSecret",
              "type": "string"
            },
            "password": {
              "default": "mychart",
              "description": "Password for the custom user to create. Ignored if postgresql.auth.existingSecret is provided",
              "required": [],
              "title": "password",
