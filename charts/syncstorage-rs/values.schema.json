  "required": [
    "nameOverride",
    "fullnameOverride",
    "replicaCount",
    "revisionHistoryLimit",
    "image",
    "imagePullSecrets",
    "serviceAccount",
    "podAnnotations",
    "podSecurityContext",
    "securityContext",
    "service",
    "ingress",
    "route",
    "resources",
    "autoscaling",
    "livenessProbe",
    "readinessProbe",
    "nodeSelector",
    "tolerations",
    "affinity",
    "extraEnv",
    "syncstorage",
    "syncserver-db",
    "tokenserver-db"
