{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "properties": {
    "fullnameOverride": {
      "default": "",
      "description": "String to fully override `\"priority-classes.fullname\"`",
      "title": "fullnameOverride",
      "type": "string"
    },
    "global": {
      "description": "Global values are values that can be accessed from any chart or subchart by exactly the same name.",
      "required": [],
      "title": "global",
      "type": "object"
    },
    "nameOverride": {
      "default": "",
      "description": "Provide a name in place of `priority-classes`",
      "title": "nameOverride",
      "type": "string"
    },
    "priorityClasses": {
      "description": "List of Priority Classes to be created",
      "items": {
        "anyOf": [
          {
            "properties": {
              "description": {
                "default": "Mission critical Pods. Does preempt lower priority Pods.",
                "title": "description",
                "type": "string"
              },
              "globalDefault": {
                "default": false,
                "title": "globalDefault",
                "type": "boolean"
              },
              "name": {
                "default": "critical",
                "title": "name",
                "type": "string"
              },
              "preemptionPolicy": {
                "default": "PreemptLowerPriority",
                "title": "preemptionPolicy",
                "type": "string"
              },
              "value": {
                "default": 1000000,
                "title": "value",
                "type": "integer"
              }
            },
            "required": [
              "name",
              "description",
              "globalDefault",
              "preemptionPolicy",
              "value"
            ],
            "type": "object"
          },
          {
            "properties": {
              "description": {
                "default": "High priority Pods. Does preempt lower priority Pods.",
                "title": "description",
                "type": "string"
              },
              "globalDefault": {
                "default": false,
                "title": "globalDefault",
                "type": "boolean"
              },
              "name": {
                "default": "high",
                "title": "name",
                "type": "string"
              },
              "preemptionPolicy": {
                "default": "PreemptLowerPriority",
                "title": "preemptionPolicy",
                "type": "string"
              },
              "value": {
                "default": 900000,
                "title": "value",
                "type": "integer"
              }
            },
            "required": [
              "name",
              "description",
              "globalDefault",
              "preemptionPolicy",
              "value"
            ],
            "type": "object"
          },
          {
            "properties": {
              "description": {
                "default": "Default priority class for all Pods. Does preempt lower priority Pods.",
                "title": "description",
                "type": "string"
              },
              "globalDefault": {
                "default": true,
                "title": "globalDefault",
                "type": "boolean"
              },
              "name": {
                "default": "medium",
                "title": "name",
                "type": "string"
              },
              "preemptionPolicy": {
                "default": "PreemptLowerPriority",
                "title": "preemptionPolicy",
                "type": "string"
              },
              "value": {
                "default": 500000,
                "title": "value",
                "type": "integer"
              }
            },
            "required": [
              "name",
              "description",
              "globalDefault",
              "preemptionPolicy",
              "value"
            ],
            "type": "object"
          },
          {
            "properties": {
              "description": {
                "default": "Low priority Pods.",
                "title": "description",
                "type": "string"
              },
              "globalDefault": {
                "default": false,
                "title": "globalDefault",
                "type": "boolean"
              },
              "name": {
                "default": "low",
                "title": "name",
                "type": "string"
              },
              "preemptionPolicy": {
                "default": "Never",
                "title": "preemptionPolicy",
                "type": "string"
              },
              "value": {
                "default": 100000,
                "title": "value",
                "type": "integer"
              }
            },
            "required": [
              "name",
              "description",
              "globalDefault",
              "preemptionPolicy",
              "value"
            ],
            "type": "object"
          }
        ],
        "required": []
      },
      "title": "priorityClasses",
      "type": "array"
    }
  },
  "required": [
    "nameOverride",
    "fullnameOverride",
    "priorityClasses"
  ],
  "type": "object"
}
