    "externalDatabase": {
      "properties": {
        "auth": {
          "properties": {
            "database": {
              "default": "mychart",
              "description": "Name of the database to use",
              "required": [],
              "title": "database",
              "type": "string"
            },
            "password": {
              "default": "mychart",
              "description": "Password to use",
              "required": [],
              "title": "password",
              "type": "string"
            },
            "username": {
              "default": "mychart",
              "description": "Name of the user to use",
              "required": [],
              "title": "username",
              "type": "string"
            }
