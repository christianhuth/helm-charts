      "required": [
        "appSecret",
        "clientIpHeader",
        "cloudMode",
        "collectApiEndpoint",
        "corsMaxAge",
        "customScript",
        "debug",
        "disableBotCheck",
        "disableLogin",
        "disableTelemetry",
        "disableUpdates",
        "enableTestConsole",
        "forceSSL",
        "hostname",
        "ignoreHostname",
        "ignoredIpAddresses",
        "logQuery",
        "migration",
        "removeDisableLoginEnv",
        "removeTrailingSlash",
        "trackerScriptName"
      ],
      "title": "umami",
      "type": "object"
    }
