          "title": "pullPolicy",
          "type": "string"
        },
        "registry": {
          "default": "docker.io",
          "description": "image registry",
          "title": "registry",
          "type": "string"
        },
        "repository": {
          "default": "martinhelmich/typo3",
          "description": "image repository",
          "title": "repository",
          "type": "string"
        },
        "tag": {
          "default": "12.4",
          "description": "Overrides the image tag",
          "title": "tag",
          "type": "string"
        }
      },
      "required": [
        "registry",
        "repository",
        "pullPolicy",
        "tag"
      ],
