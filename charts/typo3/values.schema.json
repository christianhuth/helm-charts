        "minReplicas": {
          "default": 1,
          "title": "minReplicas",
          "type": "integer"
        },
        "targetCPUUtilizationPercentage": {
          "default": 80,
          "title": "targetCPUUtilizationPercentage",
          "type": "integer"
        }
      },
      "required": [
        "enabled",
        "minReplicas",
        "maxReplicas",
        "targetCPUUtilizationPercentage"
      ],
      "title": "autoscaling",
      "type": "object"
    },
    "externalDatabase": {
      "properties": {
        "auth": {
          "properties": {
            "database": {
              "default": "typo3",
              "description": "Name of the database to use",
              "title": "database",
              "type": "string"
            },
            "existingSecret": {
              "default": "",
              "description": "Name of existing secret to use for database credentials",
              "title": "existingSecret",
              "type": "string"
            },
            "password": {
              "default": "typo3",
              "description": "Password to use",
              "title": "password",
              "type": "string"
            },
            "userPasswordKey": {
              "default": "",
              "description": "Key in the secret containing the password",
              "title": "userPasswordKey",
              "type": "string"
