    "podMonitor": {
      "properties": {
        "enabled": {
          "default": false,
          "description": "Enable a prometheus PodMonitor CR",
          "title": "enabled",
          "type": "boolean"
        },
        "selfMonitor": {
          "properties": {
            "additionalLabels": {
              "description": "Prometheus PodMonitor labels",
              "required": [],
              "title": "additionalLabels",
              "type": "object"
            },
            "enabled": {
              "default": true,
              "description": "Enable a prometheus PodMonitor to monitor the DNS Exporter",
              "title": "enabled",
              "type": "boolean"
            },
            "interval": {
              "default": "30s",
              "description": "prometheus: kube-prometheus\nPrometheus PodMonitor interval",
              "title": "interval",
              "type": "string"
            },
            "metricRelabelings": {
              "description": "Prometheus [MetricRelabelConfigs] to apply to samples before ingestion",
              "items": {
                "required": []
              },
              "title": "metricRelabelings",
              "type": "array"
            },
            "namespace": {
              "default": "",
              "description": "Prometheus PodMonitor namespace",
              "title": "namespace",
              "type": "string"
            },
            "selector": {
              "description": "release: prometheus\nPrometheus PodMonitor selector",
              "required": [],
              "title": "selector",
              "type": "object"
            }
