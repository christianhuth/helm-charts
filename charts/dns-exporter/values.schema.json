        "type": {
          "default": "prometheus",
          "description": "Which api version for the podmonitor should be used. Valid values are google and prometheus",
          "title": "type",
          "type": "string"
        }
      },
      "required": [
        "enabled",
        "type",
        "selfMonitor",
        "targetsMonitor"
      ],
      "title": "podMonitor",
      "type": "object"
    },
    "podSecurityContext": {
      "description": "pod-level security context",
      "required": [],
      "title": "podSecurityContext",
      "type": "object"
    },
    "priorityClassName": {
      "default": "",
      "description": "Priority class name\nref: https://kubernetes.io/docs/concepts/configuration/pod-priority-preemption/#priorityclass",
      "title": "priorityClassName",
      "type": "string"
    },
    "replicaCount": {
      "default": 1,
      "description": "Number of replicas",
      "title": "replicaCount",
      "type": "integer"
